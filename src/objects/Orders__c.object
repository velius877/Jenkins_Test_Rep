    <fields>
        <fullName>Calculated_Discount__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL(Payment_Type__c, &apos;Paypal&apos;), Order_Total__c - Order_Sub_Total__c - Tax__c - Calculated_Shipping__c ,
IF( ISPICKVAL(Sales_Channel__c, &apos;Phone&apos;), -Discount__c ,
IF( Authorization_Amount__c=0 , 0,
Authorization_Amount__c -Order_Sub_Total__c - Tax__c - Calculated_Shipping__c) ) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Calculated Discount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Calculated_Order_Total__c</fullName>
        <externalId>false</externalId>
        <formula>IF( AND(ISPICKVAL(Sales_Channel__c , &apos;Brand Website&apos;), NOT(Authorization_Amount__c==0 )  )  ,  Authorization_Amount__c ,  Order_Total__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Calculated Order Total</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Calculated_Shipping__c</fullName>
        <externalId>false</externalId>
        <formula>Misc_Charge_Amount__c +  Shipping_Total__c</formula>
