    <validationRules>
        <fullName>CC_Token</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
ISPICKVAL(Payment_Type__c, &quot;Credit Card&quot;) ,
NOT(ISPICKVAL(Order_Status__c, &quot;Internal Review&quot;)),
ISBLANK( Credit_Card_Token__c ))</errorConditionFormula>
        <errorDisplayField>Payment_Type__c</errorDisplayField>
        <errorMessage>If the Payment Type is Credit Card, there must be a valid Credit Card Token present in order to submit an order.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancelled_Reason</fullName>
        <active>true</active>
        <description>When Order Status is &quot;Cancelled&quot; the system will require a Cancelled Reason to be entered.</description>
        <errorConditionFormula>AND(
ISPICKVAL (Order_Status__c , &quot;Cancelled&quot;),
ISBLANK(TEXT( Cancelled_Reason__c ) ) )</errorConditionFormula>
        <errorDisplayField>Order_Status__c</errorDisplayField>
        <errorMessage>When Order Status is &quot;Cancelled&quot; there must be a Cancelled Reason entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Internal_Review_Reason</fullName>
        <active>true</active>
        <description>When Order Status is &quot;Internal Review&quot; the system will require an Internal Review Reason to be entered.</description>
        <errorConditionFormula>AND(
ISPICKVAL (Order_Status__c , &quot;Internal Review&quot;),
ISBLANK(TEXT( Internal_Review_Reason__c ) ) )</errorConditionFormula>
        <errorDisplayField>Order_Status__c</errorDisplayField>
        <errorMessage>When Order Status is &quot;Internal Review&quot; there must be an Internal Review Reason entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>International_Shipping</fullName>
        <active>false</active>
        <description>If order is being shipped Internationally, shipping method must be 2nd day air or Airmail</description>
        <errorConditionFormula>AND (
International_Address__c  = &quot;Yes&quot;,
 OR ( 
NOT( ISPICKVAL ( Shipping_Method__c , &quot;2nd Day Air&quot;) ),
NOT( ISPICKVAL ( Shipping_Method__c , &quot;Air Mail&quot;) ) ) )</errorConditionFormula>
        <errorDisplayField>Shipping_Method__c</errorDisplayField>
        <errorMessage>International Orders must be shipped by either 2nd Day Air or Airmail</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PO_Box_Shipping_Method</fullName>
        <active>true</active>
        <description>If the order is being shipped to a PO Box the shipping method must be USPS.</description>
        <errorConditionFormula>AND(
 ISPICKVAL( Sales_Channel__c , &quot;Phone&quot;) ,
 ISPICKVAL ( Ship_To_Address__r.Country_Picklist__c , &quot;UNITED STATES&quot;),
(Ship_To_PO_BOX__c = &quot;YES&quot;),
NOT(  ISPICKVAL(Shipping_Method__c, &quot;USPS&quot;) ) )</errorConditionFormula>
        <errorDisplayField>Shipping_Method__c</errorDisplayField>
        <errorMessage>If an order is being sent to a PO BOX, the shipping method must be USPS</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Return_Reason</fullName>
        <active>true</active>
        <description>When Return? is checked the system will require a Return Reason to be entered.</description>
        <errorConditionFormula>AND(
(Return__c = TRUE),
ISBLANK( Return_Reason__c ) )</errorConditionFormula>
        <errorDisplayField>Return__c</errorDisplayField>
        <errorMessage>When &quot;Return?&quot; is checked there must be a Return Reason entered.</errorMessage>
    </validationRules>
</CustomObject>
