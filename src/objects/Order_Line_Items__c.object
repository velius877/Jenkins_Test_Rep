    <validationRules>
        <fullName>Override_Reason</fullName>
        <active>true</active>
        <description>If a CSR checks override and overrides a price, they must provide a reason in the override reason field.</description>
        <errorConditionFormula>AND(
 NOT(ISBLANK(Price_Override__c)), Price_Override__c &gt; 0,
ISBLANK(TEXT(Override_Reason__c)))</errorConditionFormula>
        <errorDisplayField>Override_Reason__c</errorDisplayField>
        <errorMessage>The system requires an &quot;Override Reason&quot; before submitting an override to the price of this order.</errorMessage>
    </validationRules>
