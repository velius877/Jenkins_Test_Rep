    <validationRules>
        <fullName>Address_Lines_NULL</fullName>
        <active>true</active>
        <description>The address lines 1-3 cannot all be blank.</description>
        <errorConditionFormula>AND(
 Address_Line_1__c = NULL,
 Address_Line_2__c = NULL,
 Address_Line_3__c = NULL
)</errorConditionFormula>
        <errorDisplayField>Address_Line_1__c</errorDisplayField>
        <errorMessage>The address must have a value on lines 1, 2, or 3.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Canada_State_Code</fullName>
        <active>true</active>
        <description>Validates the Province Code for Canadian Addresses</description>
        <errorConditionFormula>IF( 
(AND(
Country__c = &quot;CA&quot;, Country__c = &quot;CAN&quot;, Country__c = &quot;CANADA&quot;) ),
( NOT (OR (State_Providence__c = &quot;AB&quot;,State_Providence__c = &quot;BC&quot;, State_Providence__c = &quot;MB&quot;, State_Providence__c = &quot;NB&quot;, State_Providence__c = &quot;NL&quot;, State_Providence__c = &quot;NT&quot;, State_Providence__c = &quot;NS&quot;, State_Providence__c = &quot;NU&quot;, State_Providence__c = &quot;ON&quot;, State_Providence__c = &quot;PE&quot;, State_Providence__c = &quot;QC&quot;, State_Providence__c = &quot;SK&quot;, State_Providence__c = &quot;YT&quot;))), NULL)</errorConditionFormula>
        <errorDisplayField>State_Providence__c</errorDisplayField>
        <errorMessage>For Canadian Addresses, there needs to be a valid 2 digit Province</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>City_NULL</fullName>
        <active>true</active>
        <description>The City cannot be blank for USA address.</description>
        <errorConditionFormula>AND(City__c = NULL,
    OR (Country__c = NULL, Country__c = &quot;US&quot;, Country__c = &quot;USA&quot;, Country__c = &quot;United States&quot;, Country__c = &quot;UNITED STATES,UNITES STATES OF AMERICA&quot;))</errorConditionFormula>
        <errorDisplayField>City__c</errorDisplayField>
        <errorMessage>The City cannot be blank for USA address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Country_NULL</fullName>
        <active>false</active>
        <description>The Address must have a country.</description>
        <errorConditionFormula>Country__c = NULL</errorConditionFormula>
        <errorDisplayField>Country__c</errorDisplayField>
        <errorMessage>The Address must include a Country</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Primary_Billing</fullName>
        <active>true</active>
        <description>If an address is designated &quot;Primary Billing&quot; Billing must be selected as address type.</description>
